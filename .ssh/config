Host github.com
  Hostname github.com
  User git
  AddKeysToAgent yes
  UserKnownHostsFile ~/.ssh/known_hosts
  IdentityFile ~/.ssh/github_rsa
  IdentitiesOnly yes
Host polytunnel.gsej.co.uk
  HostName polytunnel.gsej.co.uk
  User pi
  AddKeysToAgent yes
  UserKnownHostsFile ~/.ssh/known_hosts
  IdentityFile ~/.ssh/polytunnelpi_rsa
  IdentitiesOnly yes
Host 192.168.1.6
  HostName 192.168.1.6
  User pi
  AddKeysToAgent yes
  UserKnownHostsFile ~/.ssh/known_hosts
  IdentityFile ~/.ssh/polytunnelpi_rsa
  IdentitiesOnly yes
Host ssh.dev.azure.com
  Hostname ssh.dev.azure.com
  User git
  AddKeysToAgent yes
  UserKnownHostsFile ~/.ssh/known_hosts
  IdentityFile ~/.ssh/ceracare_rsa
  IdentitiesOnly yes
  HostKeyAlgorithms=+ssh-rsa
